    "baseUrl": "./",
    "paths": {
      "@whatworks/analytics": ["../src/index.ts"],
      "@whatworks/analytics/api/consent": ["../src/api/consent.ts"]
    },
    "noEmit": true,
    "emitDeclarationOnly": false
  }
}
